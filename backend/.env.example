GRID_API_KEY=your_key_here
GROQ_API_KEY=your_key_here
JINA_API_KEY=your_key_here
DEBUG_MODE=true

# API mode: graphql | central-data | rest
GRID_API_MODE=graphql
GRID_FALLBACK_TO_REST=false

# GraphQL configuration
GRID_GRAPHQL_URL=https://api-op.grid.gg/central-data/graphql
GRID_GRAPHQL_URLS=
GRID_GRAPHQL_QUERY_PATH=
GRID_INCLUDE_PLAYER_TEAM=true
GRID_SERIES_PAGE_SIZE=50
GRID_SERIES_MAX_PAGES=6
GRID_STATS_GRAPHQL_URL=
GRID_STATS_GRAPHQL_URLS=
GRID_SERIES_STATE_GRAPHQL_URL=https://api-op.grid.gg/live-data-feed/series-state/graphql

# Central Data Feed configuration
GRID_CENTRAL_DATA_URL=https://api.grid.gg/central-data
GRID_CENTRAL_MATCHES_PATH=/matches
GRID_CENTRAL_MATCH_PATH=/matches/{match_id}
GRID_CENTRAL_TEAMS_PATH=/teams
GRID_CENTRAL_TEAM_PARAM=teamName
GRID_CENTRAL_GAME_PARAM=gameTitle
GRID_CENTRAL_LIMIT_PARAM=limit
GRID_CENTRAL_ORDER_PARAM=orderBy
GRID_CENTRAL_ORDER_VALUE=startTime:desc

# Stats Feed configuration
GRID_STATS_FEED_URL=https://api.grid.gg/stats
GRID_STATS_MATCH_PATH=/matches/{match_id}/stats

# File Download configuration
GRID_FILE_DOWNLOAD_URL=https://api.grid.gg/file-download
GRID_END_STATE_PATH=/end-state/grid/series/{series_id}
GRID_FILE_DOWNLOAD_LIST_PATH=/list/{series_id}
GRID_FILE_DOWNLOAD_EVENTS_PATH=/events/grid/series/{series_id}

# Events enrichment
GRID_INCLUDE_EVENTS=true
GRID_EVENTS_MAX_SERIES=12

# Live Data Feed configuration
GRID_LIVE_FEED_URL=https://api.grid.gg/live
GRID_LIVE_SERIES_STATE_PATH=/series/{series_id}/state
GRID_LIVE_SERIES_EVENTS_PATH=/series/{series_id}/events

# Auth header controls (override if GRID uses x-api-key)
GRID_API_AUTH_MODE=auto
GRID_API_AUTH_HEADER=x-api-key
GRID_API_TOKEN_PREFIX=

# Optional seeding controls
GRID_MATCH_LIMIT=50
GRID_GAME_TITLE=VALORANT

# Probe script IDs
GRID_TEST_SERIES_ID=
GRID_TEST_MATCH_ID=
GRID_TEST_TEAM_ID=83
GRID_TEST_PLAYER_ID=244
GRID_TEST_ORG_ID=1
GRID_TEST_TOURNAMENT_IDS=1,2

# RAG
RAG_USE_WEB=false
RAG_URLS=
